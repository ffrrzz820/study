function hm_bbpuip_get_uploaded_image_count(){if(!tinymce||!tinymce.activeEditor||!hm_bbpuip_uploaded_image_pattern)return!1;for(var c=[],b=jQuery(tinymce.activeEditor.contentDocument).find("img"),a=0;a<b.length;++a)hm_bbpuip_uploaded_image_pattern.exec(b[a].src)&&c.push(b[a].src.toLowerCase());return jQuery.unique(c).length};
